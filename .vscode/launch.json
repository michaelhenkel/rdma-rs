    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'client'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=client",
                    "--package=client"
                ],
                "filter": {
                    "name": "client",
                    "kind": "bin"
                }
            },
            "args": [
                "--server",
                "10.87.73.1",
                "-p",
                "12345",
                "-m",
                "4096B",
                "-i",
                "1",
                "-v",
                "65536B",
                "-q",
                "2"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'server'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=server",
                    "--package=rdma-rs"
                ],
                "filter": {
                    "name": "server",
                    "kind": "bin"
                }
            },
            "args": [
                "-a",
                "10.87.73.1",
                "-p",
                "12345"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'server'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=server",
                    "--package=server"
                ],
                "filter": {
                    "name": "server",
                    "kind": "bin"
                }
            },
            "args": [
                "-a",
                "10.87.73.1",
                "-p",
                "12345"
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}